<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<DialogPane prefHeight="261.0" prefWidth="379.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.mesexadmin.PopUpController">
   <header>
      <Label style="-fx-padding: 10;" text="Update User Information">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
   </header>
   <content>
      <AnchorPane prefHeight="500.0" prefWidth="600.0" style="-fx-padding: 5;">
         <children>
            <Label layoutX="12.0" layoutY="51.0" text="Name" />
            <TextField fx:id="displayNameField" layoutX="150.0" layoutY="47.0" prefHeight="26.0" prefWidth="216.0" promptText="Display Name" />
            <Label layoutX="12.0" layoutY="94.0" text="Address" />
            <TextField fx:id="address" layoutX="150.0" layoutY="90.0" prefHeight="26.0" prefWidth="216.0" promptText="Enter Address" />
            <Label layoutX="12.0" layoutY="137.0" text="Date of birth" />
            <Label layoutX="12.0" layoutY="180.0" text="Gender" />
            <DatePicker fx:id="datePicker" layoutX="151.0" layoutY="133.0" prefHeight="26.0" prefWidth="216.0" />
            <Label layoutX="12.0" layoutY="5.0" text="Username" />
            <TextField fx:id="usernameField" disable="true" editable="false" layoutX="150.0" layoutY="1.0" prefHeight="26.0" prefWidth="216.0" />
            <RadioButton fx:id="isMale" layoutX="151.0" layoutY="180.0" mnemonicParsing="false" text="Male" />
            <RadioButton fx:id="isFemale" layoutX="278.0" layoutY="180.0" mnemonicParsing="false" text="Female" />
         </children>
      </AnchorPane>
   </content>
</DialogPane>
